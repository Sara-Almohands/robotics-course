const int pwm = 3 ;     //initializing pin 2 as ‘pwm’ variable
void setup()
{
     pinMode(pwm,OUTPUT) ;  //Set pin 2 as output
}
void loop()
{
     analogWrite(pwm,10) ;     //setting pwm to 25 
     delay(100) ;      //delay of 50 ms
      analogWrite(pwm,20) ;  
     delay(100) ;  analogWrite(pwm,30) ;  
     delay(100) ;  analogWrite(pwm,40) ;  
     delay(100) ;  analogWrite(pwm,50) ;  
     delay(100) ;  analogWrite(pwm,60) ;  
     delay(100) ;  analogWrite(pwm,70) ;  
     delay(100) ;  analogWrite(pwm,80) ;  
     delay(100) ;  analogWrite(pwm,90) ;  
     delay(100) ;  analogWrite(pwm,100) ;  
     delay(100) ;  analogWrite(pwm,110) ;  
     delay(100) ;  analogWrite(pwm,120) ;  
     delay(100) ; 
     analogWrite(pwm,130) ;  
     delay(100) ; 
     analogWrite(pwm,140) ; 
     delay(100) ; 
     analogWrite(pwm,150) ; 
     delay(100) ; 
     analogWrite(pwm,160) ; 
     delay(100) ; 
     analogWrite(pwm,170) ; 
     delay(100) ; 
     analogWrite(pwm,180) ; 
     delay(100) ;
     analogWrite(pwm,190) ; 
     delay(100) ; 
     analogWrite(pwm,200) ; 
     delay(100) ; 
     analogWrite(pwm,210) ; 
     delay(100) ;  analogWrite(pwm,220) ;  
     delay(100) ;  analogWrite(pwm,230) ;  
     delay(100) ;  analogWrite(pwm,240) ;  
     delay(100) ;  analogWrite(pwm,250) ;  
     delay(100) ; 
     
}
